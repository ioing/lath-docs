{
  "name": "lath-docs",
  "version": "0.0.1",
  "description": "Seamless connection of pages.",
  "keywords": [
    "lath",
    "web-portal",
    "pages-link",
    "linkup",
    "seamless",
    "iframe"
  ],
  "files": [
    "dist/",
    "docs/"
  ],
  "scripts": {
    "clean": "rimraf ./docs",
    "lint": "tslint --project tsconfig.json --format stylish",
    "build": "npm run marked && npm run clean && webpack build && node ./pwa.js",
    "debug": "node --inspect-brk dist/index.js",
    "start": "webpack serve --mode development",
    "marked": "node ./marked.js"
  },
  "dependencies": {
    "@types/marked": "^4.0.8",
    "@types/node": "^10.12.12",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "lath": "^0.10.22",
    "marked": "^4.2.4"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.3.2",
    "ip": "^1.1.5",
    "lodash.merge": "^4.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "text-loader": "0.0.1",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.7.3"
  },
  "author": "lien",
  "license": "MIT"
}
